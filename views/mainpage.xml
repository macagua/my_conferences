<?xml version="1.0"?>
<odoo>
    <!-- conference tree view -->
    <record id="view_tree_myconferences" model="ir.ui.view">
        <field name="name">Conferences Tree</field>
        <field name="model">myconferences.conference</field>
        <field name="arch" type="xml">
            <tree string="Conferences">
                <field name="name"/>
                <field name="start_date"/>
                <field name="end_date"/>
                <field name="fee"/>
            </tree>
        </field>
    </record>
</odoo>

